      </trans-unit>
      <trans-unit id="validAliasCharacters.label" resname="validAliasCharacters.label">
        <source>Valid alias characters</source>
      </trans-unit>
      <trans-unit id="validAliasCharacters.description" resname="validAliasCharacters.description">
